{
  "name": "@modusoperandi/licit-video",
  "version": "0.0.1",
  "subversion": "1",
  "description": "Video plugin built with ProseMirror",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": ""
  },
  "bundleDependencies": [],
  "scripts": {
    "clean": "rm -rf dist/ coverage/ bin/ && rm -f modusoperandi-licit-video-*.*.*.tgz",
    "build:ts": "tsc -p tsconfig.json",
    "build:babel": "babel src --out-dir dist --extensions .ts,.tsx",
    "build:dist": "npm run clean && npm run build:ts && npm run webpack && npm run build:babel && npm run build:css",
    "webpack": "webpack",
    "build:css": "cp src/ui/*.css dist/ui",
    "lint:ts": "eslint src --ext .ts,.tsx --fix",
    "lint:css": "stylelint \"**/*.css\" --fix",
    "prepare": "npm run build:dist",
    "test": "jest --coverage",
    "debug": "node --debug-brk --inspect ./node_modules/.bin/jest -i"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-decorators": "^7.16.0",
    "@babel/plugin-proposal-do-expressions": "^7.16.0",
    "@babel/plugin-proposal-export-default-from": "^7.16.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.16.0",
    "@babel/plugin-proposal-function-sent": "^7.16.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.16.0",
